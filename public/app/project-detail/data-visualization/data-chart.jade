.well
    .visual(ng-controller='DataChartController as data')
        ig-data-chart(id='dataChart1' data-source='data.dataChart')
            axes
                axis(name='NameAxis' type='categoryX' title='Country' label='timeStamp')
                axis(name='PopulationAxis' type='numericY' minimumvalue='0' title='Milions of People')
            series
                series(name='2015Population' type='line' is-highlight-enabled='true' is-transition-in-enabled='true' x-axis='NameAxis' y-axis='PopulationAxis' value-member-path='data')
        ig-zoombar(target='#dataChart1')
